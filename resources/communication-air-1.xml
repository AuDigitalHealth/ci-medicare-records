  <fhirVersion value="3.0.2" />
  <mapping>
    <identity value="workflow" />
    <uri value="http://hl7.org/fhir/workflow" />
    <name value="Workflow Mapping" />
  </mapping>
  <mapping>
    <identity value="w5" />
    <uri value="http://hl7.org/fhir/w5" />
    <name value="W5 Mapping" />
  </mapping>
  <mapping>
    <identity value="rim" />
    <uri value="http://hl7.org/v3" />
    <name value="RIM Mapping" />
  </mapping>
  <kind value="resource" />
  <abstract value="false" />
  <type value="Communication" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Communication" />
  <derivation value="constraint" />
  <differential>
    <element id="Communication">
      <path value="Communication" />
      <short value="Australian Immunisation Register Notice" />
      <constraint>
        <key value="inv-dh-com-01" />
        <severity value="error" />
        <human value="The subject shall at least have a reference or an identifier with at least a system and a value" />
        <expression value="subject.reference.exists() or subject.identifier.where(system.count() + value.count() &gt;1).exists()" />
      </constraint>
    </element>
    <element id="Communication.meta">
      <path value="Communication.meta" />
      <min value="1" />
      <constraint>
        <key value="inv-dh-com-02" />
        <severity value="error" />
        <human value="One profile shall be 'http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/communication-air-1'" />
        <expression value="meta.profile.where($this='http://ns.electronichealth.net.au/ci/fhir/StructureDefinition/communication-air-1').exists()" />
      </constraint>
    </element>
    <element id="Communication.meta.profile">
      <path value="Communication.meta.profile" />
      <min value="1" />
    </element>
    <element id="Communication.status">
      <path value="Communication.status" />
      <fixedCode value="completed" />
    </element>
    <element id="Communication.category">
      <path value="Communication.category" />
      <min value="1" />
      <max value="1" />
      <patternCodeableConcept>
        <coding>
          <system value="http://snomed.info/sct" />
          <code value="1500551000168103" />
        </coding>
      </patternCodeableConcept>
      <mustSupport value="true" />
    </element>
    <element id="Communication.subject">
      <path value="Communication.subject" />
      <min value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ns.electronichealth.net.au/ci/fhir/3.0/StructureDefinition/patient-ihi-1" />
      </type>
      <mustSupport value="true" />
    </element>
    <element id="Communication.payload">
      <path value="Communication.payload" />
      <mustSupport value="true" />
    </element>
    <element id="Communication.payload.content[x]">
      <path value="Communication.payload.content[x]" />
      <short value="Notice text" />
    </element>
  </differential>
